        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>two and a half to four metres</xs:documentation>
                    <xs:documentation>Definition: Significant wave heights between 2.5 and 4 metres (8-13 ft) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>four to six metres</xs:documentation>
                    <xs:documentation>Definition: Significant wave heights between 4 and 6 metres (13-20 ft) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>six to nine metres</xs:documentation>
                    <xs:documentation>Definition: Significant wave heights between 6 and 9 metres (20-30 ft) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>nine to fourteen metres</xs:documentation>
                    <xs:documentation>Definition: Significant wave heights between 9 and 14 metres (30-45 ft) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>greater than fourteen metres</xs:documentation>
                    <xs:documentation>Definition: Significant wave heights  greater than 14 metres (>45 ft) are forecast or occurring.</xs:documentation>
