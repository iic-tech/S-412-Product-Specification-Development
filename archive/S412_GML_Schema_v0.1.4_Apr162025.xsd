        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>freezing spray</xs:documentation>
                    <xs:documentation>Definition: Water droplets transported through air at below freezing temperatures.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>freezing rain</xs:documentation>
                    <xs:documentation>Definition: Precipitation drops freezing on impact to form a coating of clear ice (glaze) on the ground and on exposed objects.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>freezing fog</xs:documentation>
                    <xs:documentation>Definition: Tiny, supercooled liquid water droplets in fog can freeze instantly on exposed surfaces when surface temps are at or below freezing.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>other icing source</xs:documentation>
                    <xs:documentation>Definition: Source of icing is unknown or is different from other types of icing sources.</xs:documentation>
                </xs:annotation>
