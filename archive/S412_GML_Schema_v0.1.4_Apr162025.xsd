            <xs:documentation>Reference: WMO No.558</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Near Gale Force</xs:documentation>
                    <xs:documentation>Definition: Wind speeds between 28 and 33 knots (Beaufort scale wind force 7) are forecast to occur or are presently occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Gale Force</xs:documentation>
                    <xs:documentation>Definition: Wind speeds between 34 and 47 knots (Beaufort scale wind force 8 and 9) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Storm Force</xs:documentation>
                    <xs:documentation>Definition: Wind speeds between 48 and 63 knots (Beaufort scale wind forces 10 or 11) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Hurricane Force</xs:documentation>
                    <xs:documentation>Definition: Sustained wind speeds 64 knots or higher (Beaufort scale wind force 12) are forecast or occurring.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
