    <!-- =============================================== -->
    <!--            Namespace Information                -->
    <!-- =============================================== -->

    <xs:annotation>
        <xs:documentation>Application schema for S412 Marine Weather Warnings overlay dataset</xs:documentation>
    </xs:annotation>

    <xs:import namespace="http://www.iho.int/s100gml/5.0" schemaLocation="s100gmlbase.xsd"/>
    <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="./S100_gmlProfile.xsd"/>
    <xs:import namespace="http://www.iho.int/S-100/profile/s100_gmlProfile" schemaLocation="S100_gmlProfileLevels.xsd"/>

    <!-- =============================================== -->
    <!--                 Common Types                    -->
    <!-- =============================================== -->

    <xs:simpleType name="ISO639-3">
	<xs:annotation>
	    <xs:documentation>stub for ISO 639-3 language codes</xs:documentation>
	</xs:annotation>
	<xs:restriction base="xs:string">
	    <xs:pattern value="\w{3}"/>
	</xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ISO3166-1">
	<xs:annotation>
            <xs:documentation>stub for ISO 3166-1 country codes</xs:documentation>
	</xs:annotation>
	<xs:restriction base="xs:string">
            <xs:pattern value="\w{3}"/>
	</xs:restriction>
    </xs:simpleType>

    <!-- ================================================== -->
    <!--        Spatial Property Convenience Types          -->
    <!-- ================================================== -->

    <xs:complexType name="GM_Surface">
	<xs:choice>
	    <xs:element ref="S100:surfaceProperty"/>
	</xs:choice>
    </xs:complexType>

    <!-- ================================================== -->
    <!--                  Feature Types                     -->
    <!-- ================================================== -->

    <xs:element name="weatherWarning" type="weatherWarningType" abstract="true" substitutionGroup="gml:AbstractFeature"/>
    <xs:complexType name="weatherWarningType" abstract="true">
        <xs:annotation>
            <xs:documentation>Weather Warning</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when a specific weather scenario or event that threatens people or property meets criteria defined by the WMO (WMO No. 558 and No. 471) or by a national authority where applicable.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractFeatureType">
                <xs:sequence>
                    <!--  Warning sub-types  -->
                    <xs:element name="iceAccretionWarning" type="iceAccretionWarningType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="restrictedVisibilityWarning" type="restrictedVisibilityWarningType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="significantWaveHeightWarning" type="significantWaveHeightWarningType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="thunderstormWarning" type="thunderstormWarningType" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="windWarning" type="windWarningType" minOccurs="0" maxOccurs="unbounded"/>
                    <!--  Attributes  -->
                    <xs:element name="cancellationDate" type="cancellationDate" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="dateTimeRange" type="dateTimeRange" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="issuedDateTime" type="issuedDateTime" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="nextUpdateDateTime" type="nextUpdateDateTime" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="weatherWarningMessage" type="weatherWarningMessage" minOccurs="1" maxOccurs="unbounded"/>
                    <xs:element name="weatherWarningNumber" type="weatherWarningNumber" minOccurs="1" maxOccurs="1"/>
                    <!--  Association to DataProvider  -->
                    <!--  <xs:element name="informationProvidedFor" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/>  -->
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="windWarning" type="windWarningType" abstract="false" substitutionGroup="WeatherWarningType"/>
    <xs:complexType name="windWarningType" abstract="false">
        <xs:annotation>
            <xs:documentation>Wind Warning</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when high wind speeds pose a hazard or are life-threatening.</xs:documentation>
            <xs:documentation>Reference: WMO-No. 558, Appendix I.2, Multilingual List of terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Reference: WMO-No. 471, Annex 2.B, Multilingual List of Terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="weatherWarningType">
                <xs:sequence>
                    <xs:element name="beaufortForce" type="beaufortForce" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="windWarningThreshold" type="windWarningThreshold" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="maximumWindSpeed" type="maximumWindSpeed" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
			    <xs:choice>
			        <xs:element ref="S100:surfaceProperty"/>
			    </xs:choice>
			</xs:complexType>
		    </xs:element> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="significantWaveHeightWarning" type="significantWaveHeightWarningType" abstract="false" substitutionGroup="WeatherWarningType"/>
    <xs:complexType name="significantWaveHeightWarningType" abstract="false">
        <xs:annotation>
            <xs:documentation>"Significant Wave Height Warning"</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when sea state conditions pose a hazard or are life-threatening.</xs:documentation>
            <xs:documentation>Reference: WMO-No. 182, International Meteorological Vocabulary</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="weatherWarningType">
                <xs:sequence>
                    <xs:element name="significantWaveHeightWarningThreshold" type="significantWaveHeightWarningThreshold" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
			    <xs:choice>
			        <xs:element ref="S100:surfaceProperty"/>
			    </xs:choice>
			</xs:complexType>
		    </xs:element> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="iceAccretionWarning" type="iceAccretionWarningType" abstract="false" substitutionGroup="WeatherWarningType"/>
    <xs:complexType name="iceAccretionWarningType" abstract="false">
        <xs:annotation>
            <xs:documentation>"Ice Accretion Warning"</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when ice accumulation rates pose a hazard to vessels.</xs:documentation>
            <xs:documentation>Reference: WMO-No. 471, Annex 2.B, Multilingual List of Terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Reference: WMO-No. WMO-No. 558, Appendix I.2, Multilingual List of terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="weatherWarningType">
                <xs:sequence>
                    <xs:element name="iceAccretionRate" type="iceAccretionRate" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="sourceOfIcing" type="sourceOfIcing" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
			    <xs:choice>
			        <xs:element ref="S100:surfaceProperty"/>
			    </xs:choice>
			</xs:complexType>
                    </xs:element> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="thunderstormWarning" type="thunderstormWarningType" abstract="false" substitutionGroup="WeatherWarningType"/>
    <xs:complexType name="thunderstormWarningType" abstract="false">
        <xs:annotation>
            <xs:documentation>"Thunderstorm Warning"</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when an area of thunderstorms or squalls poses a hazard. Conditions could include strong winds, locally higher waves, potentially heavy precipitation, hail, and dangerous lightning strikes.</xs:documentation>
            <xs:documentation>Reference: </xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="weatherWarningType">
                <xs:sequence>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
			    <xs:choice>
			        <xs:element ref="S100:surfaceProperty"/>
			    </xs:choice>
			</xs:complexType>
		    </xs:element> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="restrictedVisibilityWarning" type="restrictedVisibilityWarningType" abstract="false" substitutionGroup="WeatherWarningType"/>
    <xs:complexType name="restrictedVisibilityWarningType" abstract="false">
        <xs:annotation>
            <xs:documentation>"Restricted Visibility Warning"</xs:documentation>
            <xs:documentation>Definition: A product issued by a national authority when horizontal visibility is degraded to less than 0.5 nautical miles by fog, smoke, dust, haze, heavy precipitation, or any other phenomena.</xs:documentation>
            <xs:documentation>Reference: WMO-No. 471, Annex 2.B, Multilingual List of Terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Reference: WMO-No.  558, Appendix I.2, Multilingual List of terms used in Weather and Sea Bulletins</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="weatherWarningType">
                <xs:sequence>
                    <xs:element name="geometry" maxOccurs="unbounded">
                        <xs:complexType>
			    <xs:choice>
			        <xs:element ref="S100:surfaceProperty"/>
			    </xs:choice>
			</xs:complexType>
		    </xs:element> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!-- ================================================== -->
    <!--                Information Types                   -->
    <!-- ================================================== -->

    <xs:element name="dataProvider" type="dataProviderType" substitutionGroup="S100:AbstractInformationType"/> 
    <xs:complexType name="dataProviderType">
        <xs:annotation>
            <xs:documentation>Data Provider</xs:documentation>
            <xs:documentation>Definition: The national authority providing data in an official capacity.</xs:documentation>
            <xs:documentation>Remarks: Exactly one instance of this information type per dataset</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="S100:AbstractInformationType"> 
                <xs:sequence>
                    <xs:element name="contactInformation" type="contactInformation" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="issuingService" type="issuingService" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="preparationService" type="preparationService" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="dataProviderInformation" type="gml:ReferenceType" minOccurs="1" maxOccurs="1"/> 
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <!-- ================================================== -->
    <!--                Complex Attributes                  -->
    <!-- ================================================== -->

    <xs:complexType name="contactInformation">
        <xs:annotation>
            <xs:documentation>Contact Information</xs:documentation>
            <xs:documentation>Definition: Information about where inquires can be sent.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="mailingAddress" type="mailingAddress" minOccurs="0" maxOccurs="1"/>
            <xs:element name="telephoneNumber" type="telephoneNumber" minOccurs="0" maxOccurs="1"/>
            <xs:element name="emailAddress" type="emailAddress" minOccurs="0" maxOccurs="1"/>
            <xs:element name="agencyName" type="agencyName" minOccurs="1" maxOccurs="1"/>
            <xs:element name="webAddress" type="webAddress" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="dateTimeRange">
        <xs:annotation>
            <xs:documentation>Date Time Range</xs:documentation>
            <xs:documentation>Definition: The range of time a feature is valid for.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="dateTimeStart" type="dateTimeStart" minOccurs="1" maxOccurs="1"/>
            <xs:element name="dateTimeEnd" type="dateTimeEnd" minOccurs="1" maxOccurs="1"/>
            <xs:documentation>Reference: "S-412 WMO Weather Product Specification"</xs:documentation>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="issuingService">
        <xs:annotation>
            <xs:documentation>Issuing Service</xs:documentation>
            <xs:documentation>Definition: The number of the METAREA, name of the Nation, and the National Meteorological and Hydrological Service (NMHS) or National Authority that broadcast the warning.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="countryName" type="countryName" minOccurs="1" maxOccurs="1"/>
            <xs:element name="issuingServiceName" type="issuingServiceName" minOccurs="1" maxOccurs="1"/>
            <xs:element name="mETAREAName" type="mETAREAName" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="issuingServiceName">
        <xs:annotation>
            <xs:documentation>Issuing Service Name</xs:documentation>
            <xs:documentation>Definition: The identifier that indicates which organization issued meteorological or oceanographic data.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="languageText" type="languageText" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="languageText">
        <xs:annotation>
            <xs:documentation>Language Text</xs:documentation>            
            <xs:documentation>Definition: A combination of written text and a language indication.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="language" type="language" minOccurs="1" maxOccurs="1"/>
            <xs:element name="text" type="text"  minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="maximumWindSpeed">
        <xs:annotation>
            <xs:documentation>Maximum Wind Speed</xs:documentation>
            <xs:documentation>Definition: When applied to a warning or forecast polygon, refers to the maximum wind speed or range of wind speeds found within the contour of the polygon.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="languageText" type="languageText" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="preparationService">
        <xs:annotation>
            <xs:documentation>Preparation Service</xs:documentation>
            <xs:documentation>Definition: The name of the Nation and National Meteorological and Hydrological Service (NMHS) or National Authority that prepared the warning and transferred it to the relevant Issuing Service for broadcast if not the same.</xs:documentation>
            <xs:documentation>Reference: S-412 WMO Weather Product Specification </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="countryName" type="countryName" minOccurs="1" maxOccurs="1"/>
            <xs:element name="issuingServiceName" type="issuingServiceName"  minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="weatherWarningMessage">
        <xs:annotation>
            <xs:documentation>Weather Warning Message</xs:documentation>
            <xs:documentation>Definition: Text message associated with a weather warning polygon.</xs:documentation>
            <xs:documentation>Remarks: </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <!-- subattributes -->
            <xs:element name="languageText" type="languageText" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

    <!-- ================================================== -->
    <!--                Simple Attributes                   -->
    <!-- ================================================== -->

    <xs:simpleType name="agencyName">
        <xs:annotation>
            <xs:documentation>Agency Name</xs:documentation>
            <xs:documentation>Definition: The name of an agency, entity or organization.</xs:documentation>
            <xs:documentation>Reference: S-412 WMO Weather Product Specification</xs:documentation>
            <xs:documentation>Remarks: Maximum of 300 characters.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"></xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="beaufortForce">
        <xs:annotation>
	    <xs:documentation>Beaufort Force</xs:documentation>
            <xs:documentation>Definition: Wind force scale, originally based on the state of the sea, expressed in numbers from 0 to 12, as defined by WMO (WMO 306 Manual on Codes, Volume I.1 - International Codes).</xs:documentation>
            <xs:documentation>Reference: WMO-No. 182, B0620</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Light Air</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 1 and 3 knots (Beaufort scale wind force 1).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
           <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Light Breeze</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 4 and 6 knots (Beaufort scale wind force 2).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Gentle Breeze</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 7 and 10 knots (Beaufort scale wind force 3).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Moderate Breeze</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 11 and 16 knots (Beaufort scale wind force 4).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>Fresh Breeze</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 17 and 21 knots (Beaufort scale wind force 5).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation>Strong Breeze </xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 22 and 27 knots (Beaufort scale wind force 6).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="7">
                <xs:annotation>
                    <xs:documentation>Near Gale</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 28 and 33 knots (Beaufort scale wind force 7).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="8">
                <xs:annotation>
                    <xs:documentation>Gale</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 34 and 40 knots (Beaufort scale wind force 8).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
 	    <xs:enumeration value="9">
                <xs:annotation>
                    <xs:documentation>Strong Gale</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 41 and 47 knots (Beaufort scale wind force 9).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="10">	    
                <xs:annotation>
                    <xs:documentation>Storm</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 48 and 55 knots (Beaufort scale wind force 10).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="11">
                <xs:annotation>
                    <xs:documentation>Violent Storm</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed between 56 and 63 knots (Beaufort scale wind force 11).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="12">
                <xs:annotation>
                    <xs:documentation>Hurricane</xs:documentation>
                    <xs:documentation>Definition: (1) Term, derived from a Caribbean word, first applied to tropical cyclones of the Caribbean Sea. (2) Wind with a speed of 64 knots or higher (Beaufort scale wind force 12).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
	    <xs:enumeration value="99">
                <xs:annotation>
                    <xs:documentation>Calm</xs:documentation>
                    <xs:documentation>Definition: Wind with a speed zero or less than 2 knots (Beaufort scale wind force 0). The state of sea when there are no waves.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="cancellationDate">
        <xs:annotation>
            <xs:documentation>Definition: Date and time of cancelling a notice or warning.</xs:documentation>
            <xs:documentation>Reference: "IHO WWNWS-SC"</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime">
            <xs:pattern value="[0-9][0-9][0-9][0-9][0-1][0-9][0-3][0-9]T[0-5][0-9][0-5][0-9][0-5][0-9]"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="countryName">  
        <xs:annotation>
            <xs:documentation>Country</xs:documentation>
            <xs:documentation>Definition: The name of a nation.</xs:documentation> 
            <xs:documentation>Reference: American Heritage Dictionary of the English Language </xs:documentation>
        </xs:annotation>
        <xs:restriction base="ISO3166-1"></xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="dateTimeEnd">
        <xs:annotation>
            <xs:documentation>Date Time End</xs:documentation>
            <xs:documentation>Definition: The end of a time range, expressed in Universal Time Coordinated (UTC).</xs:documentation>
            <xs:documentation>Reference: S-57: IHO Transfer Standard for Digital Hydrographic Data, Appendix A, Chapter 2 (2.219 TIMEND)</xs:documentation>
            <xs:documentation>Unit: Years, months, days, hours, minutes, seconds</xs:documentation> 
            <xs:documentation>Resolution: 1 second</xs:documentation>
